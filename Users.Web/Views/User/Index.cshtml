@model List<Users.Web.Models.UserListViewModel>

@{
    ViewBag.Title = "Users";
}

<h2>Users</h2>

<div>
    <table class="table">
        <thead>
        <tr>
            <td>Id</td>
            <td>Email</td>
            <td>UserName</td>
        </tr>
        </thead>
        <tbody>
        @{
            foreach (var user in Model)
            {       
                <tr>
                    <td>@user.Id</td>
                    <td>@user.Email</td>
                    <td>@user.UserName</td>
                </tr>
            }
        }
        </tbody>
    </table>
</div>